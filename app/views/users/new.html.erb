<div id="header">
    <h1>Sign Up</h1>
<%= form_for @user do |f| %>
<%= f.label :name %>
<div class="form_input"><%= f.text_field :name %></div>
<%= f.label :age %>
<div class="form_input"> <%= f.text_field :age %> </div>
<%= f.label :bio %>
<div class="form_input"><%= f.text_field :bio %> </div>
<%= f.label :password %>
<div class="form_input"><%= f.password_field :password %> </div>
<br>
<%= f.submit  "Create user" %>
<% end %>
</div> 


